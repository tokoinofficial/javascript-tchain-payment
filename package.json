{
    "main": "index.js",
    "name": "@tokoinofficial/t-chain-payment",
    "license": "Apache-2.0",
    "version": "2.0.2",
    "author": {
        "name": "tokoinofficial",
        "email": "hello@tokoin.io"
    },
    "engines": {
        "node": ">=14.16"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/tokoinofficial/t-chain-payment-js-sdk.git"
    },
    "keywords": [
        "Javascript",
        "Payment",
        "Tokoin",
        "T-Chain"
    ],
    "scripts": {
        "dev": "node_modules/webpack/bin/webpack.js --mode production --watch --progress",
        "build": "node_modules/webpack/bin/webpack.js --mode production --progress"
    },
    "dependencies": {
        "axios": "^1.1.3",
        "ethers": "^5.6.9",
        "qrcode": "^1.5.1"
    },
    "devDependencies": {
        "@babel/core": "^7.21.5",
        "@babel/eslint-parser": "^7.21.3",
        "eslint": "^8.39.0",
        "eslint-config-airbnb-base": "15.0.0",
        "eslint-plugin-import": "^2.25.2",
        "webpack": "^5.74.0",
        "webpack-cli": "^5.0.2"
    },
    "browserslist": [
        "> 1%",
        "last 2 versions",
        "not dead"
    ],
    "eslintConfig": {
        "root": true,
        "env": {
            "browser": true,
            "es6": true
        },
        "extends": [
            "airbnb-base"
        ],
        "overrides": [],
        "parser": "@babel/eslint-parser",
        "parserOptions": {
            "requireConfigFile": false,
            "babelOptions": {
                "babelrc": false,
                "configFile": false
            },
            "ecmaVersion": 2018,
            "sourceType": "module",
            "allowImportExportEverywhere": true
        },
        "rules": {
            "max-len": [
                "warn",
                120
            ],
            "indent": [
                "warn",
                4
            ],
            "no-underscore-dangle": 0,
            "object-curly-newline": [
                "error",
                {
                    "ObjectExpression": {
                        "multiline": true,
                        "consistent": true
                    },
                    "ObjectPattern": {
                        "multiline": true,
                        "consistent": true
                    },
                    "ImportDeclaration": {
                        "multiline": true,
                        "consistent": true
                    },
                    "ExportDeclaration": {
                        "multiline": true,
                        "consistent": true
                    }
                }
            ],
            "object-property-newline": [
                "error",
                {
                    "allowAllPropertiesOnSameLine": false,
                    "allowMultiplePropertiesPerLine": false
                }
            ]
        }
    }
}
